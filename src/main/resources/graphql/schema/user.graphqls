type User {
  id: ID!
  name: String!
  email: String!
  role: String!
}

extend type Query {
  users(search: String): [User]
}

extend type Mutation {
  addUser(name: String!, email: String!, role: String!): User
  updateUser(id: ID!, name: String, email: String, role: String): User
  deleteUser(id: ID!): Boolean
}
